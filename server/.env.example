# Supabase Configuration
# Get these from your Supabase project: Settings > API
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
SUPABASE_PROJECT_REF=your-project-ref

# Database Configuration (Direct PostgreSQL Connection)
# Get these from Supabase: Settings > Database > Connection string
DB_HOST=db.your-project-id.supabase.co
DB_PORT=5432
DB_NAME=postgres
DB_USER=postgres
DB_PASSWORD=your_database_password_here

# Database Pooler (Optional - for connection pooling)
# Get from Supabase: Settings > Database > Connection pooling
DB_POOLER_HOST=aws-0-us-east-2.pooler.supabase.com
DB_POOLER_PORT=6543

# Azure Maps API (for geocoding ZIP codes)
# Get from Azure Portal: Azure Maps account > Keys
AZURE_MAPS_API_KEY=your_azure_maps_subscription_key

# Golf API (golfapi.io)
# Get from golfapi.io dashboard
GOLF_API_KEY=your_golf_api_key_here

# Application Configuration
FRONTEND_URL=http://localhost:5173
CORS_ORIGINS=http://localhost:5173,https://your-production-url.com
PORT=8000

# OpenStreetMap Configuration (Optional)
# Get from https://www.openstreetmap.org/oauth2/applications
OSM_CLIENT_ID=your_client_id_here
OSM_CLIENT_SECRET=your_client_secret_here
